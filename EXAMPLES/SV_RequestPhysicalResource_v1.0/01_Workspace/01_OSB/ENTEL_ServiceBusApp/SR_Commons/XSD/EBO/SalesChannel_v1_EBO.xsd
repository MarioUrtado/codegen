		<xs:element ref="tns:orderCommercialChannel" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:payOfficeID" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:salesRegion" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:storeID" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:type" maxOccurs="1" minOccurs="0"/>
		</xs:sequence>
