	<import namespace="http://www.entel.cl/EBO/IndividualName/v1" schemaLocation="../../../SR_Commons/XSD/EBO/IndividualName_v1_EBO.xsd"/>
	<import namespace="http://www.entel.cl/EBO/MarketSegment/v1" schemaLocation="../../../SR_Commons/XSD/EBO/MarketSegment_v1_EBO.xsd"/>
	<import namespace="http://www.entel.cl/EBO/TroubleTicket/v1" schemaLocation="../../../SR_Commons/XSD/EBO/TroubleTicket_v1_EBO.xsd"/>
