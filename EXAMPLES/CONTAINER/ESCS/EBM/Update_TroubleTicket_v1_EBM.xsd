	<import namespace="http://www.entel.cl/EBO/TelephoneNumber/v1" schemaLocation="../../../SR_Commons/XSD/EBO/TelephoneNumber_v1_EBO.xsd"/>
	<import namespace="http://www.entel.cl/EBO/AbsoluteLocalLocation/v1" schemaLocation="../../../SR_Commons/XSD/EBO/AbsoluteLocalLocation_v1_EBO.xsd"/>
