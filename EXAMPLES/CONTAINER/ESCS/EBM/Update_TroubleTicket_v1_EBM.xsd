                			<element ref="ebons_troubleticket:agentComment" maxOccurs="1" minOccurs="0"/>
                			<element ref="ebons_troubleticket:comment" maxOccurs="1" minOccurs="0"/>
                			<element ref="ebons_troubleticket:commentBeforeProblem" maxOccurs="1" minOccurs="0"/>
                			<element ref="ebons_troubleticket:customerComment" maxOccurs="1" minOccurs="0"/>
                			<element ref="ebons_troubleticket:description" maxOccurs="1" minOccurs="0"/>
                			<element ref="ebons_troubleticket:externalTroubleID" maxOccurs="1" minOccurs="0"/>
                			<element ref="ebons_troubleticket:failureType" maxOccurs="1" minOccurs="0"/>
                			<element ref="ebons_troubleticket:transactionDate" maxOccurs="1" minOccurs="0"/>
                			<element ref="ebons_troubleticket:troubleStatus" maxOccurs="1" minOccurs="0"/>
