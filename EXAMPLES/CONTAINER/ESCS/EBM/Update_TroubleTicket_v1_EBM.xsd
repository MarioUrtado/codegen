                                                		</sequence>	
                                                		</complexType>	
                                                	</element>
                                                		
                                                	
                                        			
                                        		</sequence>	
                                        		</complexType>	
                                        	</element>
                                        		
                                        	
                                			
                                		</sequence>	
                                		</complexType>	
                                	</element>
                                		
                                	
                        			
                                	<element name="IndividualName" maxOccurs="1" minOccurs="0">
                                		<complexType>
                                		<sequence>
                                		<element ref="ebons_individualname:firstName" maxOccurs="1" minOccurs="0"/>
                                			<element ref="ebons_individualname:lastName" maxOccurs="1" minOccurs="0"/>
                                			<element ref="ebons_individualname:maidenName" maxOccurs="1" minOccurs="0"/>
                                			
                                		</sequence>	
                                		</complexType>	
                                	</element>
                                		
                                	
                        			
                                	<element name="MarketSegment" maxOccurs="1" minOccurs="0">
                                		<complexType>
                                		<sequence>
                                		<element ref="ebons_marketsegment:class" maxOccurs="1" minOccurs="1"/>
                                			
                                		</sequence>	
                                		</complexType>	
                                	</element>
                                		
                                	
                        			
                        		</sequence>	
                        		</complexType>	
                        	</element>
                        		
                        	
                			
                        	<element name="GeographicAddress" maxOccurs="1" minOccurs="0">
                        		<complexType>
                        		<sequence>
                        		
                                	<element name="Address" maxOccurs="1" minOccurs="0">
                                		<complexType>
                                		<sequence>
                                		<element ref="ebons_address:additionalAddressInfo" maxOccurs="1" minOccurs="0"/>
                                			<element ref="ebons_address:addressNumber" maxOccurs="1" minOccurs="0"/>
                                			<element ref="ebons_address:addressType" maxOccurs="1" minOccurs="0"/>
                                			
                                        	<element name="AbsoluteLocalLocation" maxOccurs="1" minOccurs="0">
                                        		<complexType>
                                        		<sequence>
                                        		<element ref="ebons_absolutelocallocation:x" maxOccurs="1" minOccurs="0"/>
                                        			<element ref="ebons_absolutelocallocation:y" maxOccurs="1" minOccurs="0"/>
                                        			
                                        		</sequence>	
                                        		</complexType>	
                                        	</element>
                                        		
                                        	
                                			
                                        	<element name="streetNameFull" maxOccurs="1" minOccurs="0">
                                        		<complexType>
                                        		<sequence>
                                        		<element ref="ebons_streetname:name" maxOccurs="1" minOccurs="0"/>
                                        			
                                        		</sequence>	
                                        		</complexType>	
                                        	</element>
                                        		
                                        	
                                			
                                		</sequence>	
                                		</complexType>	
                                	</element>
                                		
                                	
                        			
                                	<element name="GeographicArea" maxOccurs="1" minOccurs="0">
                                		<complexType>
                                		<sequence>
                                		<element ref="ebons_geographicarea:commune" maxOccurs="1" minOccurs="0"/>
                                			<element ref="ebons_geographicarea:region" maxOccurs="1" minOccurs="0"/>
                                			
                                		</sequence>	
                                		</complexType>	
                                	</element>
                                		
                                	
                        			
                        		</sequence>	
                        		</complexType>	
                        	</element>
                        		
                        	
                			
                        	<element name="Individual" maxOccurs="1" minOccurs="0">
                        		<complexType>
                        		<sequence>
                        		
                                	<element name="IndividualName" maxOccurs="1" minOccurs="0">
                                		<complexType>
                                		<sequence>
                                		<element ref="ebons_individualname:firstName" maxOccurs="1" minOccurs="0"/>
                                			<element ref="ebons_individualname:lastName" maxOccurs="1" minOccurs="0"/>
                                			<element ref="ebons_individualname:maidenName" maxOccurs="1" minOccurs="0"/>
                                			
                                		</sequence>	
                                		</complexType>	
                                	</element>
                                		
                                	
                        			
                        		</sequence>	
                        		</complexType>	
                        	</element>
                        		
                        	
                			
                        	<element name="ServiceRequest" maxOccurs="1" minOccurs="0">
                        		<complexType>
                        		<sequence>
                        		<element ref="ebons_servicerequest:area" maxOccurs="1" minOccurs="0"/>
                        			<element ref="ebons_servicerequest:impact" maxOccurs="1" minOccurs="0"/>
                        			<element ref="ebons_servicerequest:subArea" maxOccurs="1" minOccurs="0"/>
                        			<element ref="ebons_servicerequest:urgency" maxOccurs="1" minOccurs="0"/>
                        			
                        		</sequence>	
                        		</complexType>	
                        	</element>
                        		
                        	
                			
                        	<element name="UserNameResourceRole" maxOccurs="1" minOccurs="0">
                        		<complexType>
                        		<sequence>
                        		<element ref="ebons_usernameresourcerole:commonName" maxOccurs="1" minOccurs="0"/>
                        			
                        		</sequence>	
                        		</complexType>	
                        	</element>
                        		
                        	
                			
                		</sequence>	
                		</complexType>	
                	</element>
                		
                	
						</sequence>
											
				</complexType>								
			</element>
		</sequence>
		</complexType>
	
	
	<element name="Update_TroubleTicket_RSP" type="tns:Update_TroubleTicket_RSP_Type"/>
		<complexType name="Update_TroubleTicket_RSP_Type">
			<sequence>
				<element ref="header:ResponseHeader"/>
				<element name="Body">
					<complexType>
						<sequence/>
					</complexType>
				</element>
			</sequence>
		</complexType>
	
	<element name="Update_TroubleTicket_FRSP" type="tns:Update_TroubleTicket_FRSP_Type"/>
	<complexType name="Update_TroubleTicket_FRSP_Type">
		<sequence>
			<element ref="header:ResponseHeader"/>
			<element name="Body">
				<complexType>
					<sequence/>
				</complexType>
			</element>
		</sequence>
