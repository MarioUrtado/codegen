		<element name="Update_TroubleTicket_CRSP" type="tns:Update_TroubleTicket_CRSP_Type"/>
		<complexType name="Update_TroubleTicket_CRSP_Type">
		<sequence>
			<element ref="header:ResponseHeader"/>	
			<element name="Body">		
				<complexType>	
					<sequence>							
						
                	<element name="TroubleTicket" maxOccurs="1" minOccurs="0">
                		<complexType>
                		<sequence>
                		<element ref="ebons_troubleticket:ID" maxOccurs="1" minOccurs="0"/>
                			<element ref="ebons_troubleticket:createdDate" maxOccurs="1" minOccurs="0"/>
                			<element ref="ebons_troubleticket:externalTroubleID" maxOccurs="1" minOccurs="0"/>
                			
                		</sequence>	
                		</complexType>	
                	</element>
                		
                	
						</sequence>
											
				</complexType>								
			</element>
		</sequence>
		</complexType>
	
					
		<element name="Update_TroubleTicket_REQ" type="tns:Update_TroubleTicket_REQ_Type"/>
