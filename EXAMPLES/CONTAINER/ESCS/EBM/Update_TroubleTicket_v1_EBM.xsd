                			<element ref="ebons_troubleticket:troubleSubStatus" maxOccurs="1" minOccurs="0"/>
                			
                        	<element name="Contact" maxOccurs="1" minOccurs="0">
                        		<complexType>
                        		<sequence>
                        		
                                	<element name="alternativePhoneNumber" maxOccurs="1" minOccurs="0">
                                		<complexType>
                                		<sequence>
                                		<element ref="ebons_telephonenumber:number" maxOccurs="1" minOccurs="0"/>
                                			
                                		</sequence>	
                                		</complexType>	
                                	</element>
                                		
                                	
                        			
                                	<element name="mainPhoneNumber" maxOccurs="1" minOccurs="0">
                                		<complexType>
                                		<sequence>
                                		<element ref="ebons_telephonenumber:number" maxOccurs="1" minOccurs="0"/>
                                			
                                		</sequence>	
                                		</complexType>	
                                	</element>
                                		
                                	
                        			
                        		</sequence>	
                        		</complexType>	
                        	</element>
                        		
                        	
                			
                        	<element name="CustomerAccount" maxOccurs="1" minOccurs="0">
                        		<complexType>
                        		<sequence>
                        		
                                	<element name="Asset" maxOccurs="1" minOccurs="0">
                                		<complexType>
                                		<sequence>
                                		<element ref="ebons_asset:plan" maxOccurs="1" minOccurs="0"/>
                                			
                                        	<element name="MSISDN" maxOccurs="1" minOccurs="0">
                                        		<complexType>
                                        		<sequence>
                                        		<element ref="ebons_msisdn:SN" maxOccurs="1" minOccurs="0"/>
                                        			
                                        		</sequence>	
                                        		</complexType>	
                                        	</element>
                                        		
                                        	
                                			
                                        	<element name="PhysicalResource" maxOccurs="1" minOccurs="0">
                                        		<complexType>
                                        		<sequence>
                                        		
                                                	<element name="PhysicalResourceSpec" maxOccurs="1" minOccurs="0">
                                                		<complexType>
                                                		<sequence>
                                                		<element ref="ebons_physicalresourcespec:modelNumber" maxOccurs="1" minOccurs="0"/>
