<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema 
	xmlns="http://www.w3.org/2001/XMLSchema" 	
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://www.entel.cl/EBO/AppliedCustomerBillingChargeDetail/v1"
	xmlns:ebons_appliedcustomerbillingtaxrate="http://www.entel.cl/EBO/AppliedCustomerBillingTaxRate/v1"
	xmlns:ebons_paymentitem="http://www.entel.cl/EBO/PaymentItem/v1"
	xmlns:ebons_appliedcustomerbillingproductcharge="http://www.entel.cl/EBO/AppliedCustomerBillingProductCharge/v1"
	
	targetNamespace="http://www.entel.cl/EBO/AppliedCustomerBillingChargeDetail/v1"
	elementFormDefault="qualified" version="1.0">	

	 <!---=- start:Imports -=- -->
	<xs:import namespace="http://www.entel.cl/EBO/AppliedCustomerBillingTaxRate/v1" schemaLocation="../CustomerDomain/AppliedCustomerBillingTaxRate_v1_EBO.xsd"/>
	<xs:import namespace="http://www.entel.cl/EBO/PaymentItem/v1" schemaLocation="../CustomerDomain/PaymentItem_v1_EBO.xsd"/>
	<xs:import namespace="http://www.entel.cl/EBO/AppliedCustomerBillingProductCharge/v1" schemaLocation="../CustomerDomain/AppliedCustomerBillingProductCharge_v1_EBO.xsd"/>
	
