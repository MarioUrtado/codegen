	<!---=- start:EBO Content -=- -->
	<!---=- start:EBO root elements defs -=- -->

	<xs:element name="contract" type="xs:string"/>									
	<xs:element name="purchaseOrder" type="xs:string"/>									
	<xs:element name="appliedCustomerBillingProductCharge" type="ebons_appliedcustomerbillingproductcharge:AppliedCustomerBillingProductCharge_Type"/>									
	<xs:element name="appliedCustomerBillingTaxRate" type="ebons_appliedcustomerbillingtaxrate:AppliedCustomerBillingTaxRate_Type"/>									
	<xs:element name="paymentItem" type="ebons_paymentitem:PaymentItem_Type"/>									
	<!---=- end:EBO root elements defs -=- -->

<!---=- start:EBO complexType defs -=- -->
	<xs:element name="AppliedCustomerBillingChargeDetail" type="tns:AppliedCustomerBillingChargeDetail_Type"/>
	<xs:complexType name="AppliedCustomerBillingChargeDetail_Type">
		<xs:sequence> 
		<xs:element ref="tns:contract" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:purchaseOrder" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:appliedCustomerBillingProductCharge" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:appliedCustomerBillingTaxRate" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:paymentItem" maxOccurs="1" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>

<!---=- end:EBO complexType defs -=- -->
	<!---=- end:EBO Content -=- -->
	 
</xs:schema>	