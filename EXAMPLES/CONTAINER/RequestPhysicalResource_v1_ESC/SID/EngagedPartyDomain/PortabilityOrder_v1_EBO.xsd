		<xs:element ref="tns:internalIdentifier" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:messageDate" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:messageType" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:observations" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:operatorDestination" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:orderNumber" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:orderType" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:originOperator" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:portDays" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:portabilityConsultId" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:portabilityDescription" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:portabilityDueDateLastBilling" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:portabilityEjecutionLimitDate" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:portabilityIdConfirmation" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:portabilityIdResponse" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:portabilityObjectType" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:portabilityOperDateActive" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:portabilityProgramationLimitDate" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:portabilityRequestStatus" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:portabilityType" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:publishCode" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:regulatoryIdentifier" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:regulatoryReferenceDate" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:rejectedField" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:rejectedFiledValue" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:rejectionCode" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:rejectionDescription" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:rejectionReasonCode" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:requestCancelationID" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:requestId" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:routingStatus" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:scheduledDate" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:serviceType" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:subscriberType" maxOccurs="1" minOccurs="0"/>
		<xs:element ref="tns:subscriberTypeDestination" maxOccurs="1" minOccurs="1"/>
		<xs:element ref="tns:transactionNumber" maxOccurs="1" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>

<!---=- end:EBO complexType defs -=- -->
	<!---=- end:EBO Content -=- -->
	 
</xs:schema>	