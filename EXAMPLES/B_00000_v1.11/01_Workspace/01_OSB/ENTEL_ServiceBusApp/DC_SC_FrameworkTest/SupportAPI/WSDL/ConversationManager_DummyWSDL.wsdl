  <wsdl:message name="refreshCache_PublishResult_Message_Out">
    <wsdl:part name="Out" element="rcPubRes:RefreshCache_PublishResultRSP"/>
  </wsdl:message>
  
  <wsdl:portType name="ConversationManager_PortType">
    <wsdl:operation name="getConversationStatus">
