   <wsdl:message name="updConversationStatus_Message_Out">  
    <wsdl:part name="Out" element="updStatus:UpdStatusRSP"/>
  </wsdl:message>
  
  <wsdl:message name="getNewConversationStatus_Message_In">
    <wsdl:part name="In" element="getNewConversation:GetNewConversationREQ"/>    
