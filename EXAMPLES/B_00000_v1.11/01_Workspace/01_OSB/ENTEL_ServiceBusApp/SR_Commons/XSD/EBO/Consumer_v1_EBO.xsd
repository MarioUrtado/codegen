<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.entel.cl/EBO/Consumer/v1" xmlns:eddns_dictionary="http://www.entel.cl/EDD/Dictionary/v1" targetNamespace="http://www.entel.cl/EBO/Consumer/v1" elementFormDefault="qualified">
 <import namespace="http://www.entel.cl/EDD/Dictionary/v1" schemaLocation="../EDD/Dictionary_v1_EDD.xsd"/>
  <element name="Consumer" type="tns:Consumer_Type"/>
    <complexType name="Consumer_Type">
      <sequence>
        <element ref="tns:ConsumerName"/>
        <element ref="tns:ConsumerSurename"/>
        <element ref="tns:ConsumerAge" minOccurs="0"/>
        <element ref="tns:ConsumerStatus"/>
        <element ref="tns:ConsumerLegalType" minOccurs="0"/>
      </sequence>
    </complexType>
 <element name="ConsumerID" type="eddns_dictionary:id_SType"/>
 <element name="ConsumerName" type="eddns_dictionary:genericStringValue_SType"/>
 <element name="ConsumerSurename" type="eddns_dictionary:genericStringValue_SType"/>
 <element name="ConsumerAge" type="eddns_dictionary:age_SType"/>
 <element name="ConsumerStatus" type="tns:consumerStatus_SType"/>
 <element name="ConsumerType" type="tns:consumerType_SType"/>
 <element name="ConsumerLegalType" type="eddns_dictionary:genericStringValue_SType"/>
 <simpleType name="consumerStatus_SType">
    <restriction base="string">
      <enumeration value="Inactive"/>
      <enumeration value="Active"/>
      <enumeration value="Defaulter"/>
    </restriction>
 </simpleType>
  <simpleType name="consumerType_SType">
    <restriction base="string">
      <enumeration value="National"/>
      <enumeration value="International"/>
    </restriction>
 </simpleType>
</schema>