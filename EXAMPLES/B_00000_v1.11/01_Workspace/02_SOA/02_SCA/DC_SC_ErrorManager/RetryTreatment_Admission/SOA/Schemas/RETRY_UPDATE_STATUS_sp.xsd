<schema targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/db/sp/RETRY_UPDATE_STATUS" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:db="http://xmlns.oracle.com/pcbpel/adapter/db/sp/RETRY_UPDATE_STATUS" elementFormDefault="qualified">
   <element name="InputParameters">
      <complexType>
         <sequence>
            <element name="P_IDS" type="db:RETRYMANAGER_ID_T_CUR" db:index="1" db:type="Array" minOccurs="0" nillable="true"/>
            <element name="P_STATUS" type="db:RETRYMANAGER_STATUS_T_CUR" db:index="2" db:type="Array" minOccurs="0" nillable="true"/>
         </sequence>
      </complexType>
   </element>
   <element name="OutputParameters">
      <complexType>
         <sequence>
            <element name="P_SOURCE_ERROR_DESC" type="string" db:index="3" db:type="VARCHAR2" minOccurs="0" nillable="true"/>
            <element name="P_SOURCE_ERROR_CODE" type="string" db:index="4" db:type="VARCHAR2" minOccurs="0" nillable="true"/>
         </sequence>
      </complexType>
   </element>
   <complexType name="RETRYMANAGER_ID_T_CUR">
      <sequence>
         <element name="P_IDS_ITEM" type="decimal" db:type="NUMBER" minOccurs="0" maxOccurs="unbounded" nillable="true"/>
      </sequence>
   </complexType>
   <complexType name="RETRYMANAGER_STATUS_T_CUR">
      <sequence>
         <element name="P_STATUS_ITEM" type="db:string100" db:type="VARCHAR2" minOccurs="0" maxOccurs="unbounded" nillable="true"/>
      </sequence>
   </complexType>
   <simpleType name="string100">
      <restriction base="string">
         <maxLength value="100"/>
      </restriction>
   </simpleType>
</schema>
