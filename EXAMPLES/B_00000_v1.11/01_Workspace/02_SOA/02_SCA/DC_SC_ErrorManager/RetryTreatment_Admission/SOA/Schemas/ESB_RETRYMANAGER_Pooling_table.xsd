   <xs:complexType name="EsbRetrymanagerCollection">
      <xs:sequence>
         <xs:element name="EsbRetrymanager" type="EsbRetrymanager" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="EsbRetrymanager">
      <xs:sequence>
         <xs:element name="id" type="xs:decimal"/>
         <xs:element name="retrymanagerConfigId" type="xs:decimal" minOccurs="0"/>
         <xs:element name="errorHospitalId" type="xs:decimal" minOccurs="0" nillable="true"/>
