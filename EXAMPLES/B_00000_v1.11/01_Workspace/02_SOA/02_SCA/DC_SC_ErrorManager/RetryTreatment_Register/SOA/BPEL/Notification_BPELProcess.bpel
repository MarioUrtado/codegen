<process name="Notification_BPELProcess"
               targetNamespace="http://xmlns.oracle.com/DC_SC_ErrorManager/RetryTreatment_Register/Notification_BPELProcess"
               xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
               xmlns:ui="http://xmlns.oracle.com/soa/designer"
               xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/db/DC_SC_ErrorManager/RetryTreatment_Register/ESB_RETRY_NOTIFICATION_Polling"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/jms/DC_SC_ErrorManager/RetryTreatment_Register/JMS_Send_MessageDispatcherQueue_jms"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/db/DC_SC_ErrorManager/RetryTreatment_Register/SP_PROCESS_NOTIFICATION"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ess="http://xmlns.oracle.com/scheduler" xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/db/sp/SP_PROCESS_NOTIFICATION"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns5="http://www.entel.cl/ESO/ErrorTreatmentStatus/v1"
         xmlns:ns6="http://xmlns.oracle.com/pcbpel/adapter/jms/DC_SC_ErrorManager/RetryTreatment_Register/JMS_Send_MessageDispatcherQueue"
         xmlns:ns7="http://xmlns.oracle.com/pcbpel/adapter/db/top/ESB_RETRY_NOTIFICATION_Polling">
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <import namespace="http://xmlns.oracle.com/pcbpel/adapter/db/DC_SC_ErrorManager/RetryTreatment_Register/ESB_RETRY_NOTIFICATION_Polling"
          location="../WSDLs/ESB_RETRY_NOTIFICATION_Polling.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"
          ui:processWSDL="true"/>
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="ESB_RETRY_NOTIFICATION_Polling" partnerLinkType="ns1:ESB_RETRY_NOTIFICATION_Polling_plt"
                 myRole="ESB_RETRY_NOTIFICATION_Polling_role"/>
    <partnerLink name="SP_PROCESS_NOTIFICATION" partnerLinkType="ns3:SP_PROCESS_NOTIFICATION_plt"
                 partnerRole="SP_PROCESS_NOTIFICATION_role"/>
    <partnerLink name="JMS_Send_MessageDispatcherQueue" partnerLinkType="ns6:Produce_Message_plt" partnerRole="Produce_Message_role"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="ns1:EsbRetryNotificationCollection_msg"/>
    <variable name="Input_JMS_Send_MessageDispatcherQueue" messageType="ns6:Produce_Message_msg"/>
    <variable name="Input_SP_PROCESS_NOTIFICATION" messageType="ns3:args_in_msg"/>
    <variable name="Output_SP_PROCESS_NOTIFICATION" messageType="ns3:args_out_msg"/>
  </variables>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in Notification_BPELProcess.wsdl) -->
    <receive name="receiveInput" partnerLink="ESB_RETRY_NOTIFICATION_Polling"
             portType="ns1:ESB_RETRY_NOTIFICATION_Polling_ptt" operation="receive" variable="inputVariable" createInstance="yes"/>
    <assign name="Assign_Input_SP_PROCESS_NOTIFICATION">
      <copy>
        <from>$inputVariable.EsbRetryNotificationCollection[1]/ns7:EsbRetryNotification/ns7:transactionCheckId</from>
        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Input_SP_PROCESS_NOTIFICATION.InputParameters/ns4:P_TRANSACTION_CHECK_ID</to>
      </copy>
    </assign>
    <invoke name="Invoke_SP_PROCESS_NOTIFICATION" bpelx:invokeAsDetail="no" partnerLink="SP_PROCESS_NOTIFICATION"
            portType="ns3:SP_PROCESS_NOTIFICATION_ptt" operation="SP_PROCESS_NOTIFICATION"
            inputVariable="Input_SP_PROCESS_NOTIFICATION" outputVariable="Output_SP_PROCESS_NOTIFICATION"/>
    <if name="If1">
      <documentation>
        <![CDATA[Proccess_Notification_is_OK]]>
      </documentation>
      <condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Output_SP_PROCESS_NOTIFICATION.OutputParameters/ns4:P_SOURCE_ERROR_CODE = '00'</condition>
      <sequence>
        <assign name="Assign_Input_JMS_Send_MessageDispatcherQueue">
          <copy>
            <from>$Output_SP_PROCESS_NOTIFICATION.OutputParameters/ns4:P_ERROR_TREATMENT_ID</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Input_JMS_Send_MessageDispatcherQueue.body/ns5:Error_Treatment_Id</to>
          </copy>
          <copy>
            <from>$Output_SP_PROCESS_NOTIFICATION.OutputParameters/ns4:P_SOURCE_ERROR_DESC</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Input_JMS_Send_MessageDispatcherQueue.body/ns5:Description</to>
          </copy>
          <copy>
            <from>'OK'</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Input_JMS_Send_MessageDispatcherQueue.body/ns5:Status</to>
          </copy>
        </assign>
        <invoke name="Invoke_JMS_Send_MessageDispatcherQueue" partnerLink="JMS_Send_MessageDispatcherQueue"
                portType="ns6:Produce_Message_ptt" operation="Produce_Message" inputVariable="Input_JMS_Send_MessageDispatcherQueue"
                bpelx:invokeAsDetail="no"/>
      </sequence>
      <else>
        <documentation>
          <![CDATA[Process_Notification_has_error]]>
        </documentation>
        <empty name="Empty1"/>
      </else>
    </if>
  </sequence>
</process>