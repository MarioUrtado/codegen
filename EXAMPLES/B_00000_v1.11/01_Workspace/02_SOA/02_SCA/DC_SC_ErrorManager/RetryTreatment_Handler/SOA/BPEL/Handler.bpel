<!--
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  Oracle JDeveloper BPEL Designer 
  
  Created: Tue Jul 12 10:52:26 ART 2016
  Author:  mario.urtado
  Type: BPEL 2.0 Process
  Purpose: One Way BPEL Process
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-->
<process name="Handler"
               targetNamespace="http://xmlns.oracle.com/DC_SC_ErrorManager/RetryTreatment_Handler/Handler"
               xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
               xmlns:client="http://xmlns.oracle.com/DC_SC_ErrorManager/RetryTreatment_Handler/Handler"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
               xmlns:ui="http://xmlns.oracle.com/soa/designer"
               xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/jms/DC_SC_ErrorManager/RetryTreatment_Handler/JMS_Receive_RetryManagerQueue"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/db/DC_SC_ErrorManager/RetryTreatment_Handler/DBReference_SP_NEW_RETRY"
         xmlns:ns3="http://www.entel.cl/SC/ErrorManager/v1"
         xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/jms/DC_SC_ErrorManager/RetryTreatment_Handler/JMS_Send_MessageDispatcherQueue"
         xmlns:ns6="http://xmlns.oracle.com/pcbpel/adapter/db/sp/DBReference_SP_NEW_RETRY"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ns5="http://xmlns.oracle.com/pcbpel/adapter/db/top/ESB_RETRYMANAGER_Pooling"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ess="http://xmlns.oracle.com/scheduler" xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns7="http://www.entel.cl/SC/ErrorManager/RetryManager/retryDispatcher/v1"
         xmlns:ns8="http://www.entel.cl/ESO/ErrorTreatmentStatus/v1">
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <import namespace="http://xmlns.oracle.com/pcbpel/adapter/jms/DC_SC_ErrorManager/RetryTreatment_Handler/JMS_Receive_RetryManagerQueue"
          location="../WSDLs/JMS_Receive_RetryManagerQueue.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"
          ui:processWSDL="true"/>
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="JMS_Receive_RetryManagerQueue" partnerLinkType="ns1:Consume_Message_plt"
                 myRole="Consume_Message_role"/>
    <partnerLink name="DBReference_SP_NEW_RETRY" partnerLinkType="ns2:DBReference_SP_NEW_RETRY_plt"
                 partnerRole="DBReference_SP_NEW_RETRY_role"/>
    <partnerLink name="DReference_RetryManage_retryDispatcher"
                 partnerLinkType="ns3:DReference_RetryManage_retryDispatcher" partnerRole="RetryManager_PortType"/>
    <partnerLink name="JMS_Send_MessageDispatcherQueue" partnerLinkType="ns4:Produce_Message_plt"
                 partnerRole="Produce_Message_role"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="ns1:Consume_Message_msg"/>
    <variable name="In_InvokeNewRetry" messageType="ns2:args_in_msg"/>
    <variable name="Out_InvokeNewRetry" messageType="ns2:args_out_msg"/>
    <variable name="In_RetryDispatcher" messageType="ns3:retryDispatcher_Message_In"/>
    <variable name="Out_RetryDispatcher" messageType="ns3:retryDispatcher_Message_Out"/>
    <variable name="In_SendToMessageDispatcher" messageType="ns4:Produce_Message_msg"/>
  </variables>
  <faultHandlers>
    <catchAll>
      <sequence name="Sequence1">
        <assign name="AssignQueue_Fatal_Fail" xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
      
      
      
   <copy>
            <from>'FATAL_FAIL'</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$In_SendToMessageDispatcher.body/ns8:Status</to>
          </copy><copy>
         <from>'Se produjo un error en el ciclo de reintentos'</from>
         <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$In_SendToMessageDispatcher.body/ns8:Description</to>
      </copy><copy>
         <from>$inputVariable.body/ns5:errorTreatmentId</from>
         <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$In_SendToMessageDispatcher.body/ns8:Error_Treatment_Id</to>
      </copy></assign>
        <invoke name="Invoke_SendQueue"
                partnerLink="JMS_Send_MessageDispatcherQueue" portType="ns4:Produce_Message_ptt"
                operation="Produce_Message" bpelx:invokeAsDetail="no" inputVariable="In_SendToMessageDispatcher"/>
      </sequence>
    </catchAll>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in Handler.wsdl) -->
    <receive name="receiveInput" partnerLink="JMS_Receive_RetryManagerQueue" portType="ns1:Consume_Message_ptt"
             operation="Consume_Message" variable="inputVariable" createInstance="yes"/>
    <assign name="Assign_In_InvokeNewRetry">
      <copy>
        <from>$inputVariable.body/ns5:errorTreatmentId</from>
        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$In_InvokeNewRetry.InputParameters/ns6:P_ERROR_TREATMENT_ID</to>
      </copy>
    </assign><invoke name="Invoke_SP_NEW_RETRY" bpelx:invokeAsDetail="no" partnerLink="DBReference_SP_NEW_RETRY"
            portType="ns2:DBReference_SP_NEW_RETRY_ptt" operation="DBReference_SP_NEW_RETRY"
            inputVariable="In_InvokeNewRetry" outputVariable="Out_InvokeNewRetry"/>
    <assign name="AssignRetry" xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
      <copy>
        <from>$Out_InvokeNewRetry.OutputParameters/ns6:P_URL</from>
        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$In_RetryDispatcher.In/ns7:ServiceEndpoint/ns7:EndpointURL</to>
      </copy>
      <copy>
        <from>$Out_InvokeNewRetry.OutputParameters/ns6:P_ACTION</from>
        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$In_RetryDispatcher.In/ns7:ServiceEndpoint/ns7:SOAPAction</to>
      </copy>
      <extensionAssignOperation>
        <bpelx:append xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
          <bpelx:from>oraext:parseEscapedXML($inputVariable.body/ns5:requestBody)</bpelx:from>
          <bpelx:to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$In_RetryDispatcher.In/ns7:Payload</bpelx:to>
        </bpelx:append>
      </extensionAssignOperation>
    </assign><assign name="AssignCorrelation" xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
      <copy>
        <from>concat('|RT', $Out_InvokeNewRetry.OutputParameters/ns6:P_NUMBER_RETRY, '@', $inputVariable.body/ns5:convOriginalId )</from>
        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$In_RetryDispatcher.In/ns7:Payload/*[1]/*[1]/*[2]/@correlationID</to>
      </copy>
    </assign><invoke name="Invoke_RetryDispatcher"
            partnerLink="DReference_RetryManage_retryDispatcher" portType="ns3:RetryManager_PortType"
            operation="retryDispatcher" inputVariable="In_RetryDispatcher"
            outputVariable="Out_RetryDispatcher" bpelx:invokeAsDetail="no"/>
  </sequence>
</process>