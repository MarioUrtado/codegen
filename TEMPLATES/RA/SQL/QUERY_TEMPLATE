[INSERT]
system=\n\tBEGIN\n\t\tSELECT ESB_SYSTEM.ID INTO V_SYSTEM_ID FROM ESB_SYSTEM WHERE CODE = '%SYSTEM_CODE%';\n\t\tEXCEPTION\n\t\tWHEN NO_DATA_FOUND THEN\n\t\t\tINSERT INTO ESB_SYSTEM (ID, CODE, NAME, DESCRIPTION, RCD_STATUS)\n\t\t\tVALUES (ESB_SYSTEM_SEQ.NEXTVAL, '%SYSTEM_CODE%', '%SYSTEM_NAME%', '%SYSTEM_DESC%', '1') RETURNING ESB_SYSTEM.ID INTO V_SYSTEM_ID;\n\t\tEND;\n\tEND;

system_api=\t\nBEGIN\n\t\tSELECT ESB_SYSTEM_API.ID INTO V_SYSTEM_API FROM ESB_SYSTEM_API WHERE ESB_SYSTEM_API.CODE = '%SYSTEM_API_CODE%';\n\t\tEXCEPTION\n\t\tWHEN NO_DATA_FOUND THEN\n\t\t\tINSERT INTO ESB_SYSTEM_API (ID, SYSTEM_ID, CODE, NAME, DESCRIPTION, RCD_STATUS) \n\t\t\tVALUES (ESB_SYSTEM_API_SEQ.NEXTVAL, V_SYSTEM_ID, '%SYSTEM_API_CODE%', '%SYSTEM_API_NAME%', null, '1') RETURNING ESB_SYSTEM_API.ID INTO V_SYSTEM_API;\n\t\tEND;\n\tEND;

system_api_operation=\t\nBEGIN\n\t\tSELECT ESB_SYSTEM_API_OPERATION.ID INTO V_SYSTEM_API_OPERATION_ID FROM ESB_SYSTEM_API_OPERATION WHERE SYSTEM_API_ID = V_SYSTEM_API_ID AND NAME = '%SYSTEM_API_OPERATION_NAME%' AND VERSION = '%SYSTEM_API_OPERATION_VERSION%';\n\t\tEXCEPTION\n\t\tWHEN NO_DATA_FOUND THEN\n\t\t\tINSERT INTO ESB_SYSTEM_API_OPERATION (ID, SYSTEM_API_ID, VERSION, NAME, DESCRIPTION, RCD_STATUS)\n\t\t\tVALUES (ESB_SYSTEM_API_OPERATION_SEQ.NEXTVAL, V_SYSTEM_API, '%SYSTEM_API_OPERATION_VERSION%', '%SYSTEM_API_OPERATION_NAME%', '%SYSTEM_API_OPERATION_DESC%', '1') RETURNING ESB_SYSTEM_API_OPERATION.ID INTO V_SYSTEM_API_OPERATION_ID;\n\t\tEND;\n\tEND;

config=INSERT INTO ESB_CONFIG (ID, NAME, RCD_STATUS\n\t\t\tVALUES (ESB_CONFIG_SEQ.NEXTVAL, '%SYSTEM_API_OPERATION_CONFIG_NAME%', '1');

config_endpoint=INSERT INTO ESB_ENDPOINT (OPERATION_ID, TRANSPORT_ID, INSTANCE, CONFIG_ID, RCD_STATUS)\n\t\t\tVALUES ( ( SELECT ID FROM ESB_SYSTEM_API_OPERATION WHERE NAME = '%SYSTEM_API_OPERATION_NAME%' AND SYSTEM_API_ID = V_SYSTEM_API),  ( SELECT ID FROM ESB_ENDPOINT_TRANSPORT WHERE NAME = '%SYSTEM_API_TRANSPORT_NAME%'),  null,  ( SELECT ID FROM ESB_CONFIG WHERE NAME = '%SYSTEM_API_OPERATION_CONFIG_NAME%'),  '1');

config_list=INSERT INTO ESB_CONFIG_LIST (CONFIG_ID, PROPERTY_ID, VALUE, RCD_STATUS)\t\nVALUES (( SELECT ID FROM ESB_CONFIG WHERE NAME = '%SYSTEM_API_OPERATION_CONFIG_NAME%'), ( SELECT ID FROM ESB_CONFIG_PROPERTY WHERE NAME = '%CONFIG_PROPERTY%'), '%VALUE%', '1');
